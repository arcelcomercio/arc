    "wp:dev": "npm run wp -- --env.dev",
    "wp:prod": "npm run wp -- --env.prod",
    "dev": "npm-run-all --parallel wp:dev gulp",
    "dev:all": "npm run wp:dev -- --env.outputs='index, amp:css, dstory:css, story:css, dstory-video:css, story-video:css, dlite-story:css, lite-story:css, match-score:css, paywall:css, subscriptions:css, basic:css, story-recetas:css, specials:css, dbasic:css, polla:sass, signwall:css, resumen-2020:css, covid:css, trivias:css, amp-trivias:css, saltar-intro:css, dlite-parallax:css, lite-parallax:css, elecciones-2021:css, mlite-story:css' && gulp",
    "dev:amp": "npm run wp:dev -- --env.outputs='amp:css'",
    "dev:basic": "npm run wp:dev -- --env.outputs='basic:css, dbasic:css'",
    "dev:lite": "npm run wp:dev -- --env.outputs='lite-story:css, dlite-story:css, mlite-story:css'",
    "dev:story": "npm run wp:dev -- --env.outputs='story:css, dstory:css,story-video:css,dstory-video:css'",
    "dev:polla": "npm run wp:dev -- --env.outputs='polla:sass'",
    "dev:resumen": "npm run wp:dev -- --env.outputs='resumen-2020:css'",
    "dev:covid": "npm run wp:dev -- --env.outputs='covid:css'",
    "dev:elecciones": "npm run wp:dev -- --env.outputs='elecciones-2021:css'",
    "dev:saltar-intro": "npm run wp:dev -- --env.outputs=' saltar-intro:css'",
    "dev:trivias": "npm run wp:dev -- --env.outputs='trivias:css, amp-trivias:css'",
    "fusion": "fusion",
    "lint": "npm-run-all --parallel prettier eslint stylelint",
    "fix": "npm-run-all --parallel prettier:fix eslint:fix stylelint:fix",
    "prod": "npm-run-all --parallel wp:prod gulp",
    "prod:all": "npm run wp:prod -- --env.outputs='index, dstory:css, story:css, story-video:css, dstory-video:css, dlite-story:css, lite-story:css, match-score:css, amp:css, paywall:css, subscriptions:css, basic:css, story-recetas:css, specials:css, dbasic:css, polla:sass, signwall:css, resumen-2020:css, covid:css, trivias:css, amp-trivias:css, saltar-intro:css, dlite-parallax:css, lite-parallax:css, elecciones-2021:css, mlite-story:css' && gulp",
    "prod:amp": "npm run wp:prod -- --env.outputs='amp:css'",
    "prod:basic": "npm run wp:prod -- --env.outputs='basic:css, dbasic:css'",
    "prod:lite": "npm run wp:prod -- --env.outputs='lite-story:css, dlite-story:css, mlite-story:css'",
    "prod:story": "npm run wp:prod -- --env.outputs='dstory:css, story:css, story-video:css, dstory-video:css'",
    "prod:polla": "npm run wp:prod -- --env.outputs='polla:sass'",
    "prod:resumen": "npm run wp:prod -- --env.outputs='resumen-2020:css'",
    "prod:covid": "npm run wp:prod -- --env.outputs='covid:css'",
    "prod:saltar-intro": "npm run wp:prod -- --env.outputs='saltar-intro:css'",
    "prod:elecciones": "npm run wp:prod -- --env.outputs='elecciones-2021:css'",
    "prod:trivias": "npm run wp:prod -- --env.outputs='trivias:css, amp-trivias:css'",
    "pw:clean": "rm -rf components/features/paywall/dist",
    "pw:mv": "npm run dev:all && cp -a resources/dist components/features/paywall",
    "start": "fusion start",
    "test": "jest",
    "test:coverage": "set CI=true && jest --verbose --color --coverage --watchAll=false ./components/ ./src/ || true",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "watch": "npm run wp:dev -- --watch --env.outputs='index, dstory:css, story:css, story-video:css, dstory-video:css, dlite-story:css, lite-story:css, amp:css, paywall:css, subscriptions:css, polla:sass, signwall:css, basic:css, dbasic:css, mlite-story:css' ",
    "watch:basic": "npm run wp:dev -- --watch --env.outputs='basic:css, dbasic:css'",
    "watch:amp": "npm run wp:dev -- --watch --env.outputs='amp:css'",
    "watch:lite": "npm run wp:dev -- --watch --env.outputs='lite-story:css, dlite-story:css'",
    "watch:story": "npm run wp:dev -- --watch --env.outputs='dstory:css,story:css, story-video:css, dstory-video:css, mlite-story:css'",
    "watch:polla": "npm run wp:dev -- --watch --env.outputs='polla:sass'",
    "watch:resumen": "npm run wp:dev -- --watch --env.outputs='resumen-2020:css'",
    "watch:covid": "npm run wp:dev -- --watch --env.outputs='covid:css'",
    "watch:saltar-intro": "npm run wp:dev -- --watch --env.outputs='saltar-intro:css'",
    "watch:elecciones": "npm run wp:dev -- --watch --env.outputs='elecciones-2021:css'",
    "watch:trivias": "npm run wp:dev -- --watch --env.outputs='trivias:css, amp-trivias:css'",
    "watch:parallax": "npm run wp:dev -- --watch --env.outputs='lite-parallax:css, dlite-parallax:css'",
    "eslint": "eslint . --ext=js,jsx,ts,tsx",
    "eslint:fix": "eslint . --ext=js,jsx,ts,tsx --fix",
    "stylelint": "stylelint 'src/**/*.scss'",
    "stylelint:fix": "stylelint 'src/**/*.scss' --fix",
    "prettier": "prettier '**/*.{js,jsx,ts,tsx,json,css,scss}' --check --check-debug --ignore-unknown",
    "prettier:fix": "prettier '**/*.{js,jsx,ts,tsx,json,css,scss}' --write",
    "zip": "npm run pw:clean && npm run prod:all && npx fusion zip",
    "browserlist": "npx browserslist",
    "browserlist:up": "npx browserslist@latest --update-db"
