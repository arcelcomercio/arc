  "scripts": {
    "dev": "webpack --config ./config/webpack.config.js --env.dev && gulp ",
    "dev:all": "webpack --config ./config/webpack.config.js --env.dev --env.outputs='index, amp:css, dmobile:css,mobile:css, paywall:css' && gulp",
    "dev:amp": "webpack --config ./config/webpack.config.js --env.dev --env.amp",
    "dev:mobile": "webpack --config ./config/webpack.config.js --env.dev --env.mobile",
    "fusion": "fusion",
    "prod": "webpack --config ./config/webpack.config.js --env.prod && gulp",
    "prod:all": "webpack --config ./config/webpack.config.js --env.prod --env.outputs='index, dmobile:css,mobile:css, amp:css, paywall:css' && gulp",
    "prod:amp": "webpack --config ./config/webpack.config.js --env.prod --env.amp",
    "prod:mobile": "webpack --config ./config/webpack.config.js --env.prod --env.mobile",
    "pw:clean": "rm -rf components/features/paywall/dist",
    "pw:mv": "npm run dev:all && cp -a resources/dist components/features/paywall",
    "start": "fusion start",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "watch": "webpack --config ./config/webpack.config.js --watch --env.dev --env.outputs='index, dmobile:css,mobile:css, amp:css, paywall:css' ",
    "watch:amp": "webpack --config ./config/webpack.config.js --watch --env.dev --env.amp",
    "watch:mobile": "webpack --config ./config/webpack.config.js --watch --env.dev --env.mobile",
    "zip": "npm run pw:clean && npm run prod:all && npx fusion zip"
  },
  "jest": {
    "automock": false,
    "verbose": true,
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "globals": {
      "NODE_ENV": "test"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 85,
        "lines": 85
      }
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/components/utilities/thumbs.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/(components|content|properties)/**/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "mocks"
    ],
    "moduleDirectories": [
      "node_modules",
      "components",
      "mocks",
      "properties"
    ],
    "setupFilesAfterEnv": [
      "./setupTests.js"
    ],
    "moduleNameMapper": {
      "fusion:properties": "<rootDir>/jest/fusionProperties.js",
      "@components(.*)$": "<rootDir>/components/global-components/$1",
      "@utilities(.*)$": "<rootDir>/components/utilities/$1"
    }
  },
